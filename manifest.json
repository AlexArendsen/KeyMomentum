{
	"manifest_version": 2,
	"name": "KeyMomentum",
	"description": "Keyboard-only web browsing utility.",
	"version": "0.0",
	"permissions": [
		"activeTab"
	],
	"content_scripts":[
		{
			"matches": ["<all_urls>"],
			"js": ["lib/jquery.min.js","lib/fuse.min.js","keymomentum.js","bindings.js","custom.js"],
			"css": ["keymomentum.css"]
		}
	]
}
